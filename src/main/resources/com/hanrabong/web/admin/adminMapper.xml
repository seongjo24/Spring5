<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hanrabong.web.admin.AdminMapper">
<select id="adminAccess" parameterType="Admin" resultType="Admin">
SELECT ANUM, DEPARTMENT
FROM HADMIN
WHERE ANUM LIKE #{anum} AND APW LIKE #{apw}
</select>
</mapper>